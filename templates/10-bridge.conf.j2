{
    "cniVersion": "0.4.0",
    "name": "bridge",
    "type": "bridge",
    "bridge": "cnio0",
    "isGateway": true,
    "ipMasq": true,
    "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": "{{ pod_cidr }}"},
           {"subnet": "10.32.0.0/24"}
          ]
        ],
        "routes": [{"dst": "0.0.0.0/0"}]
    }
}
